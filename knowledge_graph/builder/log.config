{   
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
            "simple": {
                "format": "%(asctime)s - %(name)s:%(lineno)d - %(levelname)s - %(message)s"
            }
    },
    "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "NOTSET",
                "formatter": "simple",
                "stream": "ext://sys.stdout"
            },
            "info_file_handler": {
                "class": "logging.handlers.RotatingFileHandler",
                "level": "INFO",
                "formatter": "simple",
                "filename": "graphdb_builder.log",
                "maxBytes": 10485760,
                "backupCount": 20,
                "encoding": "utf8"
            }
    },
    "loggers": {
            "my_module": {
                "level": "ERROR",
                "handlers": ["console"],
                "propagate": "no"
            }
    },
    "root":{
        "level": "DEBUG",
        "handlers": ["console"]
    }
}
